{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "0.27.2",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^3.4.4",
    "sweetalert2": "^11.6.13",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --watchAll=false --collectCoverageFrom=\"src/**/*.{js,jsx}\"",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run --e2e",
    "e2e:dev": "start-server-and-test start http://localhost:3000 'cypress run --e2e'",
    "e2e:dev:open": "start-server-and-test start http://localhost:3000 'cypress open --e2e'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.js",
      "!src/index.js"
    ]
  },
  "devDependencies": {
    "cypress": "^15.6.0",
    "start-server-and-test": "^2.1.2",
    "webpack": "^4.42.0"
  }
}
